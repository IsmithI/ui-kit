{
  "name": "@ismithi/ui-kit",
  "version": "1.0.1",
  "description": "UI kit for building beautiful ui",
  "repository": "https://github.com/IsmithI/ui-kit.git",
  "files": [
    "build/lib/**"
  ],
  "scripts": {
    "dev": "webpack-dev-server --port 8000",
    "build": "webpack --mode production --progress",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"src/**/*.tsx\" \"src/**/*.jsx\"",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "smith",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/react": "^16.8.3",
    "classnames": "^2.2.6",
    "image-webpack-loader": "^4.6.0",
    "react": "latest",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.2",
    "react-portal": "^4.2.0",
    "ts-loader": "^5.3.3",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "@types/node": "^10.12.26",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "storybook-addon-jsx": "^6.0.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  }
}
